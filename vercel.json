{
    "buildCommand": "cd client && npm run build",
    "outputDirectory": "client/build",
    "installCommand": "npm install",
    "rewrites": [
        {"source": "/(.*)", "destination": "/api"},
        {"source": "/proxyPage", "destination": "/api/proxyPage"},
        {"source": "/submit", "destination": "/api/submit"}
    ]
}